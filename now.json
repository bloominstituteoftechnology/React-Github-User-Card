{
    "version": 2,
    "name": "my-app",
    "builds": [
        { 
            "src": "my-app/package.json", 
            "use": "@now/static-build", 
            "config": {"distDir": "build"} 
        }
    ],
    "routes": [
        {
             "src": "/(.*)", 
             "dest": "my-app/$1" 
        }
    ]
}